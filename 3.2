#include <iostream> // подключаем cin/cout
#include <stdio.h> 

using namespace std;

int main()
{
    setlocale(LC_ALL, "Russian");

    char txt[1000];
    int symbol = 0;
   
    cout << "Введиет строку" << endl;
    gets_s(txt);

    for (int i = 0; i < 1000; i++) {
        if (txt[i] == '\0') {
            break;
        }
        else if (txt[i] == 'f') {
            symbol++;
        }
    }

    if (symbol == 1) {
        cout << "-1" << endl;
    }
    else if (symbol == 0) {
        cout << "-2" << endl;
    }
    else {
        cout << symbol+1 << endl;
    }

    system ("pause");
}
